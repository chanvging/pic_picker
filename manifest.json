{
	"name": "Pic Picker",
	"description": "my tool to view pics",
	"version": "2.1",

	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},

	"permissions": ["tabs", "contextMenus", "http://*/*"],
	"background": {
		"scripts": ["background.js"]
	},

	"browser_action": {
		"default_title": "Click to show image",
		"default_icon": "icons/icon48.png"
	},

	"manifest_version": 2
}