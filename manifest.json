{
	"name":"Pic Picker",
	"description":"my tool to view pics",
	"version":"2.0",
	
	  "icons": {"16": "icons\\gallery16.png", 
            "48": "icons\\gallery48.png",
           "128": "icons\\gallery128.png"},

	"permissions":["tabs", "contextMenus", "http://*/*"],
	"background":{
		"scripts":["background.js"]
